    "collapsed": true
   },
   "outputs": [],
   "source": [
    "fact_memo = {}\n",
    "\n",
    "def fact(n):\n",
    "    if n < 2:\n",
    "        return 1\n",
    "    \n",
    "    fact_memo[n] = n * fact(n-1)\n",
    "    print(fact_memo)\n",
    "    \n",
    "    return fact_memo[n]"
   ]
