   "source": [
    "def sum_of_n_numbers(arr):\n",
    "    \"\"\"Sum of n numbers\"\"\"\n",
    "    sum = 0\n",
    "    for i in arr:\n",
    "        sum += i\n",
    "        \n",
    "\n",
    "    return sum"
   ]
  },
